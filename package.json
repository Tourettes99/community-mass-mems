{
  "name": "r1-memories",
  "version": "1.0.0",
  "type": "module",
  "description": "A community memories site for R1 users",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "netlify-lambda build netlify/functions",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "dependencies": {
    "@netlify/functions": "^2.8.2",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.10.0",
    "mongoose": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "netlify": "^13.1.21",
    "node-fetch": "^3.3.2",
    "serverless-http": "^3.2.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
