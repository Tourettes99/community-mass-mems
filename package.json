{
  "name": "r1-memories",
  "version": "1.0.0",
  "type": "module",
  "description": "A community memories site for R1 users",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "npm run build:functions && npm run build:client",
    "build:functions": "netlify-lambda build netlify/functions",
    "build:client": "cd client && npm install && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.697.0",
    "@aws-sdk/lib-storage": "^3.697.0",
    "@mui/icons-material": "^6.1.8",
    "@netlify/functions": "^2.8.2",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^19.6.0",
    "gif-info": "^1.0.1",
    "gifuct-js": "^2.1.2",
    "mongodb": "^6.10.0",
    "mongoose": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "music-metadata": "^10.6.0",
    "musicmetadata": "^2.0.5",
    "netlify": "^13.1.21",
    "node-fetch": "^3.3.2",
    "probe-image-size": "^7.2.3",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.5",
    "url-metadata": "^4.1.1",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "netlify-lambda": "^2.0.16"
  }
}
